ex:f71f5d03a468 a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    sh:select """

        SELECT ?class (COUNT(DISTINCT ?s) AS ?total) WHERE {
            GRAPH <http://www.wikidata.org/entity/> {
                ?s a ?class .
            }
        }
        GROUP BY ?class
        ORDER BY DESC(?total)
        
OFFSET 0
LIMIT 300
""" ;
    schema:dateCreated "2025-12-02T10:01:13.456440+00:00"^^xsd:dateTime ;
    schema:target [
        a sd:Service ;
        sd:endpoint <https://idsm.elixir-czech.cz/sparql/endpoint/idsm>
    ] .

ex:139e602aef90 a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    sh:select """

                    SELECT (COUNT(DISTINCT ?s) AS ?participating) WHERE {
                        GRAPH <http://www.wikidata.org/entity/> {
                            ?s <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ?o .
                            ?s a <http://semanticscience.org/resource/CHEMINF_000018> .
                            ?o a <http://www.w3.org/2002/07/owl#Class> .
                        }
                    }
                    
""" ;
    schema:dateCreated "2025-12-02T10:01:13.623615+00:00"^^xsd:dateTime ;
    schema:target [
        a sd:Service ;
        sd:endpoint <https://idsm.elixir-czech.cz/sparql/endpoint/idsm>
    ] .

