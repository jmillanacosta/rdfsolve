@prefix ex: <https://github.com/jmillanacosta/rdfsolve/sparql/bgee/bgee/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:621c66dce143 a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    sh:select """

        PREFIX void: <http://rdfs.org/ns/void#>
        PREFIX void-ext: <http://ldf.fi/void-ext#>
        SELECT DISTINCT ?subjectClass ?prop ?objectClass ?objectDatatype ?g
        WHERE {
          GRAPH ?g {
            {
              ?cp void:class ?subjectClass ;
                  void:propertyPartition ?pp .
              ?pp void:property ?prop .
              OPTIONAL {
                  {
                      ?pp  void:classPartition [ void:class ?objectClass ] .
                  } UNION {
                      ?pp void-ext:datatypePartition [ void-ext:datatype ?objectDatatype ] .
                  }
              }
            } UNION {
              ?ls void:subjectsTarget [ void:class ?subjectClass ] ;
                  void:linkPredicate ?prop ;
                  void:objectsTarget [ void:class ?objectClass ] .
            }
          }
        }
        
""" ;
    schema:dateCreated "2025-12-02T10:02:08.164885+00:00"^^xsd:dateTime ;
    schema:target [
        a sd:Service ;
        sd:endpoint <https://www.bgee.org/sparql>
    ] .
