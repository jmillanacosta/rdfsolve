ex:560e80b1fb4a a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    sh:select """

        SELECT ?class (COUNT(DISTINCT ?s) AS ?total) WHERE {
            GRAPH <http://wifo5-04.informatik.uni-mannheim.de/drugbank/resource/drugs/> {
                ?s a ?class .
            }
        }
        GROUP BY ?class
        ORDER BY DESC(?total)
        
OFFSET 0
LIMIT 300
""" ;
    schema:dateCreated "2025-12-02T10:01:07.919763+00:00"^^xsd:dateTime ;
    schema:target [
        a sd:Service ;
        sd:endpoint <https://idsm.elixir-czech.cz/sparql/endpoint/idsm>
    ] .

ex:dfa70aac926d a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    sh:select """

                    SELECT (COUNT(DISTINCT ?s) AS ?participating) WHERE {
                        GRAPH <http://wifo5-04.informatik.uni-mannheim.de/drugbank/resource/drugs/> {
                            ?s <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ?o .
                            ?s a <http://semanticscience.org/resource/SIO_011120> .
                            ?o a <http://www.w3.org/2002/07/owl#Class> .
                        }
                    }
                    
""" ;
    schema:dateCreated "2025-12-02T10:01:08.024356+00:00"^^xsd:dateTime ;
    schema:target [
        a sd:Service ;
        sd:endpoint <https://idsm.elixir-czech.cz/sparql/endpoint/idsm>
    ] .

